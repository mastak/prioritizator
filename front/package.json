{
  "name": "prioritizator-client",
  "version": "0.0.1",
  "description": "Client project for prioritizator",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:mastak/prioritizator.git"
  },
  "authors": [
    "Igor Lubimov <infunt@gmail.com>"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "start": "npm run build && serve .",
    "build": "npm run build-js && npm run build-css",
    "watch": "npm run watch-js & npm run watch-css & serve .",
    "test": "npm run lint -s && npm run build",
    "build-css": "rework-npm index.css | cleancss -o build/main.css",
    "build-js": "browserify --extension=.jsx --extension=.js src/app.jsx | uglifyjs > build/main.js",
    "watch-js": "watchify --extension=.jsx --extension=.js src/app.jsx -o build/main.js --debug --verbose",
    "watch-css": "nodemon -e css --ignore build/main.css --exec 'rework-npm index.css -o build/main.css'",
    "lint-eslint": "eslint .",
    "lint-jscs": "jscs .",
    "lint": "npm run lint-eslint && npm run lint-jscs"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mastak/prioritizator/issues"
  },
  "homepage": "https://github.com/mastak/prioritizator",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "flux": "^2.1.1",
    "hellojs": "^1.9.9",
    "jwt-decode": "^1.4.0",
    "react": "^0.14",
    "react-mixin": "^1.1.0",
    "react-router": "^1.0.2",
    "reqwest": "^2.0.4",
    "when": "^3.7.5"
  },
  "devDependencies": {
    "babelify": "^6.1.0",
    "browserify": "^12.0.1",
    "clean-css": "^3.4.8",
    "eslint": "^1.10",
    "nodemon": "^1.8",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "serve": "^1.4.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6"
  }
}
